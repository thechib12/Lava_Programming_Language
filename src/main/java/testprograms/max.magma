chamber max {


    rupture rock max (rock[] $a){
        rock $maximum = 0;
        rock $counter =0;
        while ($counter < $a.length()){
          if ($a[$counter] >= $maximum) then {
            $maximum = $a[$counter];
          }
          $counter = $counter + 1;
        }
        return $maximum;
    }

    erupt(){
        rock[] $array = with size 5;
        $array = [0,45,2,7,4];
        rock $x = max ($array);
/^\        explode("Maximum: "  + $x);
    }
}

